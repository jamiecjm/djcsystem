<p id="notice"><%= notice %></p>

<h1>Salevalues</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @salevalues.each do |salevalue| %>
      <tr>
        <td><%= link_to 'Show', salevalue %></td>
        <td><%= link_to 'Edit', edit_salevalue_path(salevalue) %></td>
        <td><%= link_to 'Destroy', salevalue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Salevalue', new_salevalue_path %>
